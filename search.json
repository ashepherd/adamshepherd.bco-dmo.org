---
layout: null
---
[
  {% for topic in site.topics %}
    {
      "title"    : "{{ topic.title | escape }}",
      "url"      : "{{ site.baseurl }}{{ topic.url }}",
      "content"     : {{ topic.content | strip_html | strip | jsonify}}
    } {% unless forloop.last %},{% endunless %}
  {% endfor %}
]
