---
layout: null
---
[
  {% for topic in site.topics %}
    {
      "title"    : "{{ topic.title | escape }}",
      "url"      : "{{ site.baseurl }}{{ topic.url }}",
      "content"     : "{{ topic.content | markdownify | strip_html }}"
    } {% unless forloop.last %},{% endunless %}
  {% endfor %}
]
